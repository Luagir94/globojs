{
  "name": "globojs",
  "private": false,
  "version": "1.3.0",
  "type": "module",
  "keywords": [
    "vite",
    "react",
    "typescript",
    "tailwindcss",
    "storybook"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "homepage": "https://github.com/Luagir94/globojs",
  "repository": {
    "type": "git",
    "url": "https://github.com/Luagir94/globojs"
  },
  "files": [
    "dist/"
  ],
  "release": {
    "branches": [
      "master"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "test": "jest",
    "build": "npm run clean && tsc & vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run clean:docs && storybook build",
    "commit": "git-cz",
    "clean": "rimraf dist/",
    "clean:docs": "rimraf storybook-static/",
    "push:npm": "np",
    "prepare": "husky",
    "semantic-release": "semantic-release --branches master"
  },
  "devDependencies": {
    "@react-icons/all-files": "^4.1.0",
    "@storybook/addon-actions": "^8.0.9",
    "@storybook/addon-docs": "^8.0.9",
    "@storybook/addon-essentials": "^8.0.9",
    "@storybook/addon-interactions": "^8.0.9",
    "@storybook/addon-links": "^8.0.9",
    "@storybook/addon-onboarding": "^8.0.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^8.0.9",
    "@storybook/blocks": "^8.0.9",
    "@storybook/builder-vite": "^8.0.9",
    "@storybook/manager-api": "^8.0.9",
    "@storybook/react": "^8.0.9",
    "@storybook/react-vite": "^8.0.9",
    "@storybook/test": "^8.0.9",
    "@storybook/theming": "^8.0.9",
    "@testing-library/react": "^15.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "branchsite": "^4.0.3",
    "commitizen": "^4.3.0",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "np": "^10.0.5",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^23.0.8",
    "semantic-release-cli": "^5.4.6",
    "storybook": "^8.0.9",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=12"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@storybook/addon-viewport": "^8.0.9"
  }
}
